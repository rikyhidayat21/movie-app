<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>List Cast</title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">

</head>
<body>
  <h1>List Cast</h1>
  <a href="/casts/add">add cast</a><br>

  <a href="/">back to home</a><br>
  <table border="1">
    <thead>
      <tr>
        <th>Name</th>
        <th>Birth Year</th>
        <th>Phone Number</th>
        <th>Gender</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
      <% casts.forEach(el => { %>

        <tr>
          <td><%= el.fullname()%></td>
          <td><%= el.birth_year%></td>
          <td><%= el.phone_number%></td>
          <td><%= el.gender%></td>
          <td>
            <a href="/casts/edit/<%= el.id%>">Edit</a> || 
            <a href="/casts/delete/<%= el.id%>">Delete</a> || 
            <a href="/casts/movies/<%= el.id%>">See Movies</a>
          </td>
        
          
        </tr>

      <% })%>
    </tbody>
  </table>
</body>
</html>